@article{merinopoulouCOPDExacerbationsDisease2016,
  title = {{{COPD}} Exacerbations by Disease Severity in {{England}}},
  author = {Merinopoulou, Evie and {Raluy-Callado}, Mireia and Ramagopalan, Sreeram and MacLachlan, Sharon and Khalid, Javaria Mona},
  year = {2016},
  month = apr,
  volume = {11},
