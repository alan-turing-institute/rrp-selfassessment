@article{stowNationalEarlyWarning2020,
  title = {National {{Early Warning Scores}} ({{NEWS}} / {{NEWS2}}) and {{COVID}}-19 Deaths in Care Homes: A Longitudinal Ecological Study},
  shorttitle = {National {{Early Warning Scores}} ({{NEWS}} / {{NEWS2}}) and {{COVID}}-19 Deaths in Care Homes},
