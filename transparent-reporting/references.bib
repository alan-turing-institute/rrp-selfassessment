  title = {A {{Multicenter Observational Prospective Cohort Study}} of {{Association}} of the {{Prehospital National Early Warning Score}} 2 and {{Hospital Triage}} with {{Early Mortality}}},
  author = {{Mart{\'i}n-Rodr{\'i}guez}, Francisco and {L{\'o}pez-Izquierdo}, Ra{\'u}l and {del Pozo Vegas}, Carlos and {Delgado-Benito}, Juan F. and {del Pozo P{\'e}rez}, Carmen and Carbajosa Rodr{\'i}guez, Virginia and Mayo Iscar, Agust{\'i}n and {Mart{\'i}n-Conty}, Jos{\'e} Luis and Escudero Cuadrillero, Carlos and {Castro-Villamor}, Miguel A.},
