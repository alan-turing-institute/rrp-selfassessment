@article{hungTripleCombinationInterferon2020,
  title = {Triple Combination of Interferon Beta-1b, Lopinavir\textendash Ritonavir, and Ribavirin in the Treatment of Patients Admitted to Hospital with {{COVID}}-19: An Open-Label, Randomised, Phase 2 Trial},
  shorttitle = {Triple Combination of Interferon Beta-1b, Lopinavir\textendash Ritonavir, and Ribavirin in the Treatment of Patients Admitted to Hospital with {{COVID}}-19},
  author = {Hung, Ivan Fan-Ngai and Lung, Kwok-Cheung and Tso, Eugene Yuk-Keung and Liu, Raymond and Chung, Tom Wai-Hin and Chu, Man-Yee and Ng, Yuk-Yung and Lo, Jenny and Chan, Jacky and Tam, Anthony Raymond and Shum, Hoi-Ping and Chan, Veronica and Wu, Alan Ka-Lun and Sin, Kit-Man and Leung, Wai-Shing and Law, Wai-Lam and Lung, David Christopher and Sin, Simon and Yeung, Pauline and Yip, Cyril Chik-Yan and Zhang, Ricky Ruiqi and Fung, Agnes Yim-Fong and Yan, Erica Yuen-Wing and Leung, Kit-Hang and Ip, Jonathan Daniel and Chu, Allen Wing-Ho and Chan, Wan-Mui and Ng, Anthony Chin-Ki and Lee, Rodney and Fung, Kitty and Yeung, Alwin and Wu, Tak-Chiu and Chan, Johnny Wai-Man and Yan, Wing-Wah and Chan, Wai-Ming and Chan, Jasper Fuk-Woo and Lie, Albert Kwok-Wai and Tsang, Owen Tak-Yin and Cheng, Vincent Chi-Chung and Que, Tak-Lun and Lau, Chak-Sing and Chan, Kwok-Hung and To, Kelvin Kai-Wang and Yuen, Kwok-Yung},
  year = {2020},
  month = may,
  volume = {395},
  pages = {1695--1704},
  publisher = {{Elsevier}},
  issn = {0140-6736, 1474-547X},
  doi = {10.1016/S0140-6736(20)31042-4},
  abstract = {{$<$}h2{$>$}Summary{$<$}/h2{$><$}h3{$>$}Background{$<$}/h3{$><$}p{$>$}Effective antiviral therapy is important for tackling the coronavirus disease 2019 (COVID-19) pandemic. We assessed the efficacy and safety of combined interferon beta-1b, lopinavir\textendash ritonavir, and ribavirin for treating patients with COVID-19.{$<$}/p{$><$}h3{$>$}Methods{$<$}/h3{$><$}p{$>$}This was a multicentre, prospective, open-label, randomised, phase 2 trial in adults with COVID-19 who were admitted to six hospitals in Hong Kong. Patients were randomly assigned (2:1) to a 14-day combination of lopinavir 400 mg and ritonavir 100 mg every 12 h, ribavirin 400 mg every 12 h, and three doses of 8 million international units of interferon beta-1b on alternate days (combination group) or to 14 days of lopinavir 400 mg and ritonavir 100 mg every 12 h (control group). The primary endpoint was the time to providing a nasopharyngeal swab negative for severe acute respiratory syndrome coronavirus 2 RT-PCR, and was done in the intention-to-treat population. The study is registered with ClinicalTrials.gov, NCT04276688.{$<$}/p{$><$}h3{$>$}Findings{$<$}/h3{$><$}p{$>$}Between Feb 10 and March 20, 2020, 127 patients were recruited; 86 were randomly assigned to the combination group and 41 were assigned to the control group. The median number of days from symptom onset to start of study treatment was 5 days (IQR 3\textendash 7). The combination group had a significantly shorter median time from start of study treatment to negative nasopharyngeal swab (7 days [IQR 5\textendash 11]) than the control group (12 days [8\textendash 15]; hazard ratio 4{$\cdot$}37 [95\% CI 1{$\cdot$}86\textendash 10{$\cdot$}24], p=0{$\cdot$}0010). Adverse events included self-limited nausea and diarrhoea with no difference between the two groups. One patient in the control group discontinued lopinavir\textendash ritonavir because of biochemical hepatitis. No patients died during the study.{$<$}/p{$><$}h3{$>$}Interpretation{$<$}/h3{$><$}p{$>$}Early triple antiviral therapy was safe and superior to lopinavir\textendash ritonavir alone in alleviating symptoms and shortening the duration of viral shedding and hospital stay in patients with mild to moderate COVID-19. Future clinical study of a double antiviral therapy with interferon beta-1b as a backbone is warranted.{$<$}/p{$><$}h3{$>$}Funding{$<$}/h3{$><$}p{$>$}The Shaw-Foundation, Richard and Carol Yu, May Tam Mak Mei Yin, and Sanming Project of Medicine.{$<$}/p{$>$}},
  file = {/Users/cburr/Zotero/storage/MPW5WSTH/Hung et al. - 2020 - Triple combination of interferon beta-1b, lopinavi.pdf;/Users/cburr/Zotero/storage/YT72ZSRG/fulltext.html},
  journal = {The Lancet},
  language = {English},
  number = {10238},
